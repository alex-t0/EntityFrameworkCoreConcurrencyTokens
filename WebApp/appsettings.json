{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "DbConfiguration" : {
    "ConnectionStrings": [
      {
        "Name": "Postgres",
        "Url": "Host=<pghostname>;Port=<pgPort>;Username=<username>;Password=<top-secret>;Database=<pg database name>"
      },
      {
        "Name": "MsSql",
        "Url": "user id=<ms sql user>; password=<top-secret>;server=<srv>\\<instance>;database=<db name>; encrypt=false"
      }
    ],
    "ActiveConnectionString": "Postgres"
  }
}
